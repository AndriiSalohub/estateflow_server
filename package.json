{
  "name": "estateflow_server",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "dotenv -e .production.env -- node dist/index.js",
    "dev": "dotenv -e .development.env -- nodemon --exec tsx src/index.ts",
    "dev:debug": "dotenv -e .env.development -- nodemon --exec tsx --inspect src/index.ts",
    "db:generate": "npx drizzle-kit generate",
    "db:migrate": "npx drizzle-kit migrate",
    "db:push": "npx drizzle-kit push",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.18",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.1",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.1",
    "nodemon": "^3.1.10",
    "tsx": "^4.19.4"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@types/pg": "^8.15.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "pg": "^8.15.6"
  }
}
