{
  "name": "estateflow_server",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production dotenv -e .production.env bun run build && bun run start",
    "dev": "NODE_ENV=development dotenv -e .development.env nodemon --exec tsx src/index.ts",
    "dev:debug": "nodemon --exec tsx --inspect src/index.ts",
    "build": "tsc",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsx": "^4.19.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "dotenv": "^16.5.0"
  }
}
